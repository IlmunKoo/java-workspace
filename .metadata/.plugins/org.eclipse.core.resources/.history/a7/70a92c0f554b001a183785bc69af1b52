package loopexample;

public class break_continue_example {

	public static void main(String[] args) {
		
		int num;
		int dan;
		
		for(dan = 2; dan <= 9; dan++) {
			if(dan % 2 != 0) continue
				//이 단이 짝수단이 아니면 위에 for조건문으로 갈 것임
			// 짝수단일 경우 밑의 for문으로 가서 돌 것임
			for(num = 1; num <= 9; num++) {
			
				
				System.out.println(dan + "X" + num +"="+dan*num);	
			}
			
			 
			System.out.println();
		}
	
	}
	}
	   

