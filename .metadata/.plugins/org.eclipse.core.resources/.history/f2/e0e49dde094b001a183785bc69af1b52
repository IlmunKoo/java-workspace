package ifexample;

import java.util.Scanner;

public class IfExample3 {

	public static void main(String[] args) {
	Scanner scanner = new Scanner(System.in);
	
	int score = scanner.nextInt();
	
	int grade = 0;
	
	if (0 < score <= 59) {
		grade = 'F';
	} else if (score <= 69) {
		grade = 'D';
	} else if (score <= 79) {
		grade = 'C';
	} else if (score <= 89) {
		grade = 'B';
	}
	else (90 < score <= 100){
		grade = 'A';
		}
	
	
	System.out.println("점수" = score);
	System.out.println("학점" = grade);
	
	
		

	}

}
