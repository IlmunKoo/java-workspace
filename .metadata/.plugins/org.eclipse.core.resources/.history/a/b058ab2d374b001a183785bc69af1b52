package loopexample;

public class WhileExample_짝_홀수만더하기 {

	public static void main(String[] args) {
       int num =1;
       int sum = 0;
       
       
       while (num <= 100) {
    	   //while(true) >> 무한루프
       sum += num;
       num += 2; //11이 됐을 때 빠져나오게 됨
    		        }
System.out.println(sum);
System.out.println(num);
	}

}

