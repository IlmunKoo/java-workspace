package loopexample;

public class break_continue_prac1 {

	public static void main(String[] args) {
		int count;
		int dan;
		
		for (dan = 2; dan < 10; dan++ ) {
			
			if ( dan % 2 != 0)continue;
			
			for (count = 1; count < 10; count++) {
				
		
				System.out.println(dan + "X" + count+"=" + dan*count);
				if (count >= dan) break;
								
			} System.out.println();
		}
				

	}

}
