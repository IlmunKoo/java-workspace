package loopexample;

import java.util.Scanner;

public class DoWhilePractice1 {

	public static void main(String[] args) {
		int input;
		int sum = 0;
		
		Scanner sc = new Scanner(System.in);
		input = sc.nextInt();
		
		
		while(input != 0 ) {
			sum +=input;
			input =  sc.nextInt();
			
			
		}
		
System.out.println(sum);
	}

}
